{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "company_handle",
    "title",
    "salary",
    "equity"
  ],
  "properties": {
    "company_handle": {
      "$id": "#/properties/company_handle",
      "type": "string",
      "title": "The Company_handle Schema",
      "default": "",
      "examples": [
        "RITH"
      ],
      "pattern": "^(.*)$"
    },
    "title": {
      "$id": "#/properties/title",
      "type": "string",
      "title": "The Title Schema",
      "default": "",
      "examples": [
        "Janitor"
      ],
      "pattern": "^(.*)$"
    },
    "salary": {
      "$id": "#/properties/salary",
      "type": "number",
      "title": "The Salary Schema",
      "default": 0,
      "examples": [
        100000000
      ]
    },
    "equity": {
      "$id": "#/properties/equity",
      "type": "number",
      "title": "The Equity Schema",
      "default": 0,
      "maximum": 1,
      "examples": [
        0
      ]
    }
  }
}